{
    "name": "jaydee-io/coreui-laravel",
    "description": "Quick website scaffolding for the Laravel framework using : Sass, Vue.js and CoreUI",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jérôme Dumesnil",
            "email": "jerome.dumesnil@gmail.com"
        }
    ],

    "require": {
        "php": "^8.1.0",
        "ext-json": "*",
        "illuminate/console": "^10.17",
        "illuminate/support": "^10.17"
    },
    "require-dev": {
        "inertiajs/inertia-laravel": "^0.6.11"
    },

    "extra": {
        "laravel": {
            "providers": [
                "JayDeeIO\\CoeruiLaravel\\CoeruiLaravelServiceProvider"
            ]
        }
    },

    "autoload": {
        "psr-4": {
            "JayDeeIO\\CoeruiLaravel\\": "src/"
        }
    }
}
